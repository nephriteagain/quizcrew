{
  "expo": {
    "name": "QuizCrew",
    "slug": "review",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quiz-crew-icon.png",
    "scheme": "quiz-crew",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7f65790f-a0ec-466d-a87c-b596668cdfd8",
      "requestHeaders": {
        "expo-runtime-version": "1.0.0",
        "expo-channel-name": "production"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nephritesoftware.quizcrew",
      "googleServicesFile": "GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quiz-crew-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nephritesoftware.quizcrew",
      "googleServicesFile": "google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quiz-crew-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f65790f-a0ec-466d-a87c-b596668cdfd8"
      }
    },
    "owner": "nephrite-software"

  }
}
